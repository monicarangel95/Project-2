  },
  "homepage": "https://github.com/monicarangel95/Project-2#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "sequelize": "^6.3.4",
    "mongoose": "^5.3.16",
    "morgan": "^1.9.1",
    "compression": "^1.7.4",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  }
